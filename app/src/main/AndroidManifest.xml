<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.e_itmedi">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.EITmedi"
        android:usesCleartextTraffic="true">
        <activity android:name="Navdrawer.ProfileActivity"></activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".ProductDetailsActivity" />
        <activity android:name=".Database.InsertDataActivity" />
        <activity android:name=".CartActivity" />
        <activity android:name=".Authentication.SignUpActivity" />
        <activity android:name=".Authentication.LogInActivity" />
        <activity android:name=".CategoryActivity" />
        <activity android:name=".FirstActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>